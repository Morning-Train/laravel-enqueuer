    "keywords": [
        "morningtrain",
        "enqueuer",
		"laravel",
		"css",
		"js"
    ],
    "homepage": "https://github.com/morningtrain/laravel-enqueuer",
    "license": "GNU GPL v3.0",
    "authors": [
        {
            "name": "Bjarne Bonde",
            "email": "bb@morningtrain.dk",
            "homepage": "https://morningtrain.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "morningtrain\\enqueuer\\": "src"
        }
    }
}
