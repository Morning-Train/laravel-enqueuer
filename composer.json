{
    "name": "morningtrain/enqueuer",
    "description": "Laravel package to enqueue css and js files for load. It supports dependencies, to serve files in the correct order. Caching included.",
    "keywords": [
        "morningtrain",
        "enqueuer"
    ],
    "homepage": "https://github.com/morningtrain/laravel-enqueuer",
    "license": "MIT",
    "authors": [
        {
            "name": "Bjarne Bonde",
            "email": "bb@morningtrain.dk",
            "homepage": "https://morningtrain.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "morningtrain\\enqueuer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "morningtrain\\enqueuer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
